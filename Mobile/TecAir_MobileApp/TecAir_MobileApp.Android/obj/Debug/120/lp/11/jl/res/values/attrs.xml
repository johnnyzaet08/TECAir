<resources><declare-styleable name="SfRotator"><!--  Set the color for Selected Dots  --><attr name="rotatorSelectedDotsColor" format="color" value="DarkGray" /><!--  Set the color for Unselected Dots  --><attr name="rotatorUnselectedDotsColor" format="color" value="White" /><!--  Set the Border color for Dots  --><attr name="rotatorDotsBorderColor" format="color" value="White" /><!--  Enable the AutoPlay for rotator items --><attr name="rotatorEnableAutoPlay" format="boolean" value="false" /><!--  Enable the TextVisible for rotator items --><attr name="rotatorTextVisible" format="boolean" value="false" /><!--  Enable the Looping for rotator items --><attr name="rotatorEnableLooping" format="boolean" value="true" /><!--  Enable the Swiping for rotator items --><attr name="rotatorEnableSwiping" format="boolean" value="false" /><!-- Set selectedIndex for rotator item --><attr name="rotatorSelectedIndex" format="integer" value="0" /><!-- Set NavigationDelay index for rotator item --><attr name="rotatorNavigationDelay" format="integer" value="2500" /><!-- Set the Dot depent on DotsPlacement  --><attr name="rotatorDotsPlacement" format="enum"><enum name="None" value="0" /><enum name="Default" value="1" /><enum name="OutSide" value="2" /></attr><!-- Rotator item alignment depend on NavigationDirection  --><attr name="rotatorNavigationDirection" format="enum"><enum name="Horizontal" value="0" /><enum name="Vertical" value="1" /></attr><!--  set the mode depend on NavigationstripMode --><attr name="rotatorNavigationStripMode" format="enum"><enum name="Dots" value="0" /><enum name="Thumbnail" value="1" /></attr><!-- Rotator item position depend on NavigationStripPosition  --><attr name="rotatorNavigationStripPosition" format="enum"><enum name="Top" value="0" /><enum name="Bottom" value="1" /><enum name="Left" value="2" /><enum name="Right" value="3" /></attr><!--  Rotator moving depend on playdirection --><attr name="rotatorPlayDirection" format="enum"><enum name="MoveForward" value="0" /><enum name="MoveBackward" value="1" /></attr></declare-styleable></resources>